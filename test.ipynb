   "source": [
    "def fak(x):\n",
    "    summe = 1\n",
    "    for i in range (1,x+1):\n",
    "        summe = summe * i\n",
    "    return summe"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "outputs": [],
   "source": [
    "def fak_rec(n):\n",
    "    if n <= 1:\n",
    "        return 1\n",
    "    else:   \n",
    "      return  n * fak_rec(n-1)"
   ],
